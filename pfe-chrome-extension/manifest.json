{
    "manifest_version": 3,
    "name": "PhotoFinishEdge",
    "version": "1.0",
    "permissions": [
        "webRequest",
        "*://*.photofinish.live/*",
        "storage"
    ],
    "host_permissions": [
        "https://cjsbc9npme.execute-api.us-east-2.amazonaws.com/",
        "*://photofinishedge.com/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    }
}
